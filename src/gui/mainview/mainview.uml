    JPanel buttons = new JPanel();&#xD;
    buttons.add(cancel);&#xD;
    buttons.add(random);&#xD;
    buttons.add(okay);&#xD;
    this.getContentPane().add(inputs);&#xD;
    this.getContentPane().add(buttons);&#xD;
    this.setVisible(true);&#xD;
</body>
