    this.scarIcon = ImageIO.read(new File(&quot;src/resources/icons8_Car_48px.png&quot;));&#xD;
    this.berlineIcon = ImageIO.read(new File(&quot;src/resources/icons8_Sedan_48px.png&quot;));&#xD;
