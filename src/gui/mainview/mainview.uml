            <ownedAttribute xmi:type="uml:Property" xmi:id="_5ZohQP8gEei9BtoOAyi2kw" name="destination" visibility="private" type="_35gyUP8gEei9BtoOAyi2kw" isUnique="false"/>
            <ownedAttribute xmi:type="uml:Property" xmi:id="_5ZqWcP8gEei9BtoOAyi2kw" name="time" visibility="private" type="_35gyUP8gEei9BtoOAyi2kw" isUnique="false"/>
            <ownedAttribute xmi:type="uml:Property" xmi:id="_5ZrkkP8gEei9BtoOAyi2kw" name="ridetypeselector" visibility="private" type="_36N88P8gEei9BtoOAyi2kw" isUnique="false"/>
            <ownedAttribute xmi:type="uml:Property" xmi:id="_5ZtZwP8gEei9BtoOAyi2kw" name="dialog" visibility="private" type="_32R4IP8gEei9BtoOAyi2kw" isUnique="false"/>
