            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_5ZewQP8gEei9BtoOAyi2kw" name="valueChanged" specification="_5Zc7EP8gEei9BtoOAyi2kw">
              <language>JAVA</language>
              <body>&#xD;
    if (!arg0.getValueIsAdjusting()) {&#xD;
        // gets the ID of the Ride at the given row&#xD;
        String rideID = table.getValueAt(table.getSelectedRow(), 0).toString();&#xD;
        Ride selectedRide = null;&#xD;
        for (Ride r : Environment.getInstance().getBookOfRides()) {&#xD;
            if (r.getID() == Integer.parseInt(rideID)) {&#xD;
                selectedRide = r;&#xD;
            }&#xD;
        }&#xD;
        if (selectedRide != null) {&#xD;
            new MarkRideDialog(selectedRide);&#xD;
        } else {&#xD;
            System.out.println(&quot;Error : selected Ride is not present in the Book Of Rides.&quot;);&#xD;
        }&#xD;
    }&#xD;
</body>
