          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_QNKCgOxAEeidJqNj7XZK8g" name="nearest" specification="_QNFxEOxAEeidJqNj7XZK8g">
            <language>JAVA</language>
            <body>&#xD;
    Coordinates nearest = list.get(0);&#xD;
    double min_distance = this.distance(nearest);&#xD;
    for (Coordinates coord : list) {&#xD;
        double distance = this.distance(coord);&#xD;
        if (distance &lt; min_distance) {&#xD;
            nearest = coord;&#xD;
            min_distance = distance;&#xD;
        }&#xD;
    }&#xD;
    return nearest;&#xD;
</body>
