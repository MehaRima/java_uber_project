          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_2I6oEOxAEeidJqNj7XZK8g" name="computeDuration" specification="_2I29sOxAEeidJqNj7XZK8g">
            <language>JAVA</language>
            <body>&#xD;
    double length = startingPoint.distance(destination);&#xD;
    double speed;&#xD;
    if (this.traffic == Traffic.low) {&#xD;
        speed = 0.25;&#xD;
    } else if (this.traffic == Traffic.medium) {&#xD;
        speed = 0.125;&#xD;
    } else {&#xD;
        speed = 0.05;&#xD;
    }&#xD;
    int duration = (int) (length / speed);&#xD;
    return duration;&#xD;
</body>
